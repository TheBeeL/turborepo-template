{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Astro",
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "plugins": [{ "name": "@astrojs/ts-plugin" }],
    "verbatimModuleSyntax": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "allowImportingTsExtensions": true
  }
}
